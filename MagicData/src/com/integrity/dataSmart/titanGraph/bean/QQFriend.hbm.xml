<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.integrity.dataSmart.titanGraph.bean.QQFriend" table="TB_RNS_QQ_FRIENDLIST" >
		<id name="id">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="qqNum" type="string">
            <column name="QQNUM" length="10" >
                <comment>qq号码</comment>
            </column>
        </property>
        <property name="friendNum" type="string">
            <column name="FRIENDNUM" length="10" >
                <comment>好友qq</comment>
            </column>
        </property>
        <property name="friendRemark" type="string" >
            <column name="FRIENDREMARK" length="64" >
                <comment>备注</comment>
            </column>
        </property>
	</class>
</hibernate-mapping>