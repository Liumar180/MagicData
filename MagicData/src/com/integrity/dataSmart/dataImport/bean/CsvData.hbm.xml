<?xml version="1.0" encoding="utf-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
     <class name="com.integrity.dataSmart.dataImport.bean.CsvData" table="csv_data">
         <id name="id" column="id">
             <generator class="identity" />
         </id>
         <property name="fileName" type="java.lang.String" >
         	<column name="fileName" length="200" not-null="true">
                <comment>文件名称</comment>
            </column>
         </property>
         <property name="fileEncoding" type="java.lang.String"  >
         	<column name="fileEncoding" length="200" not-null="true">
                <comment>文件编码</comment>
            </column>
         </property>
         <property name="fileSeparator" type="java.lang.String" >
         	<column name="fileSeparator" length="200" not-null="true">
                <comment>分隔符</comment>
            </column>
         </property>
         <property name="titleFlag" type="java.lang.Boolean"  >
         	<column name="titleFlag" length="200" not-null="true">
                <comment>是否有表头</comment>
            </column>
         </property>
         <property name="uploadTime" type="java.util.Date">
            <column name="uploadTime"  not-null="true">
                <comment>上传时间</comment>
            </column>
        </property>
         <property name="by1" type="java.lang.String">
            <column name="by1" length="100" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="by2" type="java.lang.String">
            <column name="by2" length="200" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="by3" type="java.lang.Integer">
            <column name="by3" length="4000" >
                <comment>备用</comment>
            </column>
        </property>
     </class>
 </hibernate-mapping>
