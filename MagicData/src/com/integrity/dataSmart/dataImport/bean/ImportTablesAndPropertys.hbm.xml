<?xml version="1.0" encoding="utf-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.integrity.dataSmart.dataImport.bean">

     <class name="ImportTablesAndPropertys" table="t_importtablesandpropertys">
         <id name="id" column="id">
             <generator class="identity" />
         </id>
         <property name="connectionName" column="connectionName" not-null="false" length="100" />
         <property name="connectionDB" column="connectionDB" not-null="false" length="100" />
         <property name="connectionDBTableName" column="connectionDBTableName" not-null="false" length="100" />
         <property name="connectionDBPropertyName" column="connectionDBPropertyName" not-null="false" length="100" />
         <property name="dbid" column="dbid" not-null="false"   length="100"/>
     </class>
 </hibernate-mapping>
