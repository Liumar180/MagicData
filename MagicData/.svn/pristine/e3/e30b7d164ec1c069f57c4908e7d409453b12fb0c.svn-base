package com.integrity.lawCase.organizationManage.wapper;

import java.util.Map;

import com.integrity.lawCase.organizationManage.service.OrgInfoManageService;

/**
 * 组织查询条件包装类
 * @author HanXue
 *
 */
public class OrgSearchPreWapper {

	/**组织查询条件包装类的类型(默认是list类型)*/
	private String wapperType  = OrgInfoManageService.SEARCH_CONDITON_LIST;
	/**列表查询条件键值对*/
	private Map<String,String> listConditionMap;
	/**列表查询条件子键值对*/
	private Map<String,Map<String,String>> listSubConditionMap;
	/**卡片查询条件键值对*/
	private Map<String,String> cardConditionMap;
	/**卡片查询条件子键值对*/
	private Map<String,Map<String,String>> cardSubConditionMap;
	
	public String getWapperType() {
		return wapperType;
	}
	public void setWapperType(String wapperType) {
		this.wapperType = wapperType;
	}
	public Map<String, String> getListConditionMap() {
		return listConditionMap;
	}
	public void setListConditionMap(Map<String, String> listConditionMap) {
		this.listConditionMap = listConditionMap;
	}
	public Map<String,Map<String,String>> getListSubConditionMap() {
		return listSubConditionMap;
	}
	public void setListSubConditionMap(Map<String,Map<String,String>> listSubConditionMap) {
		this.listSubConditionMap = listSubConditionMap;
	}
	public Map<String, String> getCardConditionMap() {
		return cardConditionMap;
	}
	public void setCardConditionMap(Map<String, String> cardConditionMap) {
		this.cardConditionMap = cardConditionMap;
	}
	public Map<String, Map<String, String>> getCardSubConditionMap() {
		return cardSubConditionMap;
	}
	public void setCardSubConditionMap(
			Map<String, Map<String, String>> cardSubConditionMap) {
		this.cardSubConditionMap = cardSubConditionMap;
	}
	
}
