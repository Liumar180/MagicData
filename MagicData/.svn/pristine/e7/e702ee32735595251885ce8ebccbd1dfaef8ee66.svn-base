<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	
	 
	<!-- 查询bean配置 -->
	<bean id="searchDetailDao" class="com.integrity.dataSmart.titanGraph.dao.SearchDetailDao" >
	</bean>
	<bean id="saveShowLabelsDao" class="com.integrity.dataSmart.titanGraph.dao.SaveShowLabelsDao" >
	</bean>
	<bean id="searchDetailService" class="com.integrity.dataSmart.titanGraph.service.SearchDetailService" >
		<property name="searchDetailDao" ref="searchDetailDao"/>
	</bean>
	<bean id="showLabelsService" class="com.integrity.dataSmart.titanGraph.service.ShowLabelsService" >
		<property name="saveShowLabelsDao" ref="saveShowLabelsDao"/>
	</bean>
	<bean id="searchDetailAction" class="com.integrity.dataSmart.titanGraph.action.SearchDetailAction" scope="prototype">
		<property name="searchDetailService" ref="searchDetailService"/>
	</bean>
	<bean id="showLabelsAction" class="com.integrity.dataSmart.titanGraph.action.ShowLabelsAction" scope="prototype">
		<property name="showLabelsService" ref="showLabelsService"/>
	</bean>
	
	<!-- 属性历史记录 -->
	<bean id="propertyHistoryDao" class="com.integrity.dataSmart.titanGraph.dao.PropertyHistoryDao">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
    <bean id="propertyHistoryService" class="com.integrity.dataSmart.titanGraph.service.PropertyHistoryService">
        <property name="propertyHistoryDao" ref="propertyHistoryDao"/>
        <property name="searchDetailService" ref="searchDetailService"/>
    </bean>
    <bean id="propertyHistoryAction" class="com.integrity.dataSmart.titanGraph.action.PropertyHistoryAction">
        <property name="propertyHistoryService" ref="propertyHistoryService"/>
    </bean>
</beans>