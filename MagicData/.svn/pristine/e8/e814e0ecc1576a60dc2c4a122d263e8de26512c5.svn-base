#login span{
	color: #ccc;
}
table  {font-size:18px;color:#ccc;}
table a {font-size:18px;color:#ccc;}
.box{
	display:none;
}
.blackBg{
	position: fixed;
	top: 0px; 
	right:0px;
	bottom:0px;
	filter: alpha(opacity=60);
	z-index: 10; 
	left: 0px; 
	background-color:#000;
	opacity:0.7; 
	-moz-opacity:0.7;
}
.box1{
	width: 40%; 
	height: 50%;
	position: fixed;
	left: 44%; 
	top: 25%;
	margin-left: -250px;
	color:#000;
	font-size:20px;
	font-family:Microsoft YaHei;
	border: 1px solid #000;
	border-radius: 6px;
	background-color:#f2f2f2;
	z-index: 999; 
}
.box1 h1{
	color: #fff;
	background: #273238;
	font-size: 16px;
	border:1px solid #273238;
	border-radius: 6px 6px 0px 0px;
	line-height: 50px;
	padding-left: 25px;
	margin-bottom: 0px;	
	text-align:left;
	margin-bottom: 20px;
}
.box1 span label{
	width:30%;
	font-size:16px;
	text-align:right;
	float:left;
	line-height:36px;
}
.box1 span input{
	width:50%;
	height:36px;
	line-height:36px;
	float:left;
	border:1px solid #ddd;
	padding:5px;
}
.box1 span input.xgBtn,
.box1 span input.closeBtn,
.box1 span input.gobackBtn,
.box1 span input.detailBtn,
.box1 span input.delBtn
{
	width:20%;
	line-height:36px;
	font-size:14px;
	padding:0px;
	color:#fff;
	background: #006ab2;
	border-radius: 6px;
	margin-left:160px;
}
.box1 span input.closeBtn,
.box1 span input.gobackBtn,
.box1 span input.detailBtn,
.box1 span input.delBtn{
	margin-left:50px;
}
.modal{
	overflow:hidden;
}
.wrapper { position:relative; width:auto; min-width:1200px; left:0px; right:0px; margin:0 auto; oveflow:hidden;}
.navmenu { width: 100%; height: 28px; overflow: hidden; margin: 0 auto;}
.navmenu .main { width: 1200px; height: auto; min-height: 30px; overflow: hidden; margin: 0 auto; }
.navmenu .main .left { width: auto; float: left; }
.navmenu .main .right { width: auto;height:30px; float: right; padding-top: 3px; }
.navmenu .main .right a { color: #ccc; font-size: 18px; padding: 5px 23px; text-decoration:none}
.navmenu .main .right label { color: #ccc; font-size: 18px; padding: 5px 2px; }
.navmenu .main .right a:hover { cursor: pointer; color:#666666}
.tableList{font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;width:100%;height:400px; border-collapse:collapse;border:1px #5599FF solid;}
.tableList td, .tableList th {font-size:1em; border:1px solid #5599FF;padding:3px 7px 2px 7px;}
.tableList tr.alt td {color:#000000;background-color:#5599FF;}
tbody a{color:#99BBFF;text-decoration:none; padding: 5px 5px;}	
tbody a:hover{ cursor: pointer; color:#666666;}
#PageCtrl_Table tr td{border:0px}
