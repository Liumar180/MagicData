.clear{
	clear: both;
}
em{
	font-style: normal;
	display: inline-block;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
/*表格超出出现省略号*/
.table tr td{
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	font-size: 14px;
}
html,body{
	width: 100%;
	height: 100%;
	font-family: "Microsoft YaHei","Arial";
	background: #f7f9fb;
	color: #3f3f3f;
}
.container{
	padding: 0px;
}
.headerBox{
	background: #000;
}
.navbar{
	min-height: 60px;
	margin-bottom: 0px;
	color: #fff;
}
.navbar-brand{
	height: 60px;
	padding-top: 18px;
}
.container{
	width: 98%;
	height: 100%;
}
.dataSpan {
    color: #fff;
    float: left;
    line-height: 60px;
    margin-left: 13px;
}
.dataSpan em {
    margin-right: 13px;
}
.navMenu{
	float: left;
	margin-left: 120px;
}
.navMenu ul{
	margin-bottom: 0px;
}
.navMenu li{
	color: #fff;
	display: inline-block;
	line-height: 60px;
	margin-right: 10px;
	padding: 0px 20px;
	cursor: pointer;
}
.navMenu li.li-seton{
	background: #0081d7;
}
/*头部结束*/
.mainCon{
	height: 90%;
	padding: 20px 0px 0px 0px;
	overflow-x: hidden;
}
.mainCon-top h3,.mainCon-top2 h3{
	font-family: "Arial";
	font-size: 16px;
	font-weight: bold;
	color: #3f3f3f;
	display: inline-block;
	margin: 0px;
}
.mainCon-top .borderSpan{
	width: 76%;
	height: 1px;
	display: inline-block;
	border: 1px dashed #d2d2d2;
	margin: 0px 16px;
}
@media (min-width: 1400px) {
  .mainCon-top .borderSpan{
		width: 88%;
	}
}
.mainCon-top .addSpan{
	display: inline-block;
	background: #0081d7;
	width: 100px;
	height: 24px;
	line-height: 24px;
	border-radius: 3px;
	text-align: center;
}
.mainCon-top .addSpan a{
	width: 90px;
	display: inline-block;
	padding-left: 14px;
	color: #fff;
	font-family: "Arial";
	background: url(../images/import/addIcon.png) 9px 6px no-repeat;
}
.mainCon-top .iconSpan{
	display: inline-block;
	margin-left: 10px;
	cursor: pointer;
}
/*第二部分*/
.mainCon-top2{
	margin-top: 10px;
}
.mainCon-top2 .borderSpan2{
	width: 88%;
	height: 1px;
	display: inline-block;
	border: 1px dashed #d2d2d2;
	margin: 0px 16px;
}
@media (min-width: 1400px) {
  .mainCon-top2 .borderSpan2{
		width: 90%;
	}
}
/*表格*/
.tableDiv{
	margin-top: 15px;
}
.tableDiv .table{
	margin-bottom: 0px;
}
.tableDiv .table > thead > tr > th{
	border-bottom: 0px;
	background: #0f4970;
	color: #fff;
}
.tableDiv .table > thead > tr > th,.tableDiv .table > tbody > tr > th,.tableDiv .table > tfoot > tr > th,.tableDiv .table > thead > tr > td,.tableDiv .table > tbody > tr > td,.tableDiv .table > tfoot > tr > td{
	border: none;
	text-align: center;
}
.tableDiv .table tbody{
	background: #6eb6e4;
	color: #fff;
}
.tableDiv .table tbody tr{
	border-bottom: 1px solid #4f98c7;
}
/*第三部分*/
.maincon-third{
	height: 68%;
	margin-top: 20px;
}
.thirdleft{
	width: 50%;
	height: 100%;
	overflow: hidden;
	position: relative;
}
.thirdleft img{
	/*width: 100%;*/
	height: 100%;
}
/*中间*/
.thirdcenter{
	width: 23%;
	margin: 0px 10px;
}
.thirdcenter h3, .thirdleft h3, #previewNodeName, #previewNodeType{
	font-family: "Arial";
	font-size: 16px;
	font-weight: bold;
	color: #3f3f3f;
	margin: 0px 0px 10px 0px;
}
