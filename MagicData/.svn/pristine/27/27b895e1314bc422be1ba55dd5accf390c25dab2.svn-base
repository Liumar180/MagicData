/*4月27日*/
/*添加数据源--csv-开始*/
.importP{
	margin-top: 20px;
}
.importSpan{
	line-height: 28px;
}
.file {
    position: relative;
    display: inline-block;
    background: #0081d7;
    border: 1px solid #0081d7;
    border-radius: 4px;
    padding: 4px 12px;
    overflow: hidden;
    color: #fff;
    text-decoration: none;
    text-indent: 0;
    line-height: 20px;
    float: left;
    cursor: pointer;
}
.file input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
}
.file:hover {
    background: #0081d7;
    border-color: #0081d7;
    color: #fff;
    text-decoration: none;
}
.fieldP{
	margin:20px 0px;
}
.csvUl li{
	width: 16%;
	float: left;
	margin-bottom: 20px;
}
.csvUl li .fieldSpan{
	width: 40%;
	display: inline-block;
}
.gobackBtn{
	background: #4092ef;
	color: #fff;
	margin-right: 30px;
}
/*添加数据源--csv-结束*/

