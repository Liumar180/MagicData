/************shade*************/
.shade{
	filter:alpha(opacity=70);  
    -moz-opacity:0.7;  
    -khtml-opacity: 0.7;  
    opacity: 0.7;  
    z-index:5;
    display:none;
    width: 100%;
	height: 100%;
	position: absolute; 
	top: 0%; 
	left: 0%;
	background-color: black;
}
