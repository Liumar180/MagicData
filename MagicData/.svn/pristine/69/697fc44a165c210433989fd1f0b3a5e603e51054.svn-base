/*index.jsp页面样式*/
body {
	background-image: url(../../images/img/bg.JPG);
	margin: 0;
	padding: 0;
	overflow:hidden;
}

input,button {
	margin: 0;
	padding: 0;
	list-style-type: none;
}

.search_div {
	float: right;
	position: absolute;
	top: 15px;
	left: 400px;
	color: #fff;
}

.search_input {
	border: 0px;
	outline: none;
	background-color: #002744;
	width: 300px;
	height: 36px;
	line-height: 36px;
	font-size:14px;
	color: #fff;
	border:1px solid #014769;
	border-right:none;
	padding:1px 0px 1px 4px;
	
}

.search_div button img {
	width: 12px;
	height: 12px;
	
}

.search_div button {
	border: 0px;
	outline: none;
	padding: 0px 11px;
	height:36px;
	background-color: #002744;
	position: absolute;
	top: 0px;
	left: 300px;
	-moz-border-radius : 0px 10px 10px 0px;
	border-radius: 0px 10px 10px 0px;
	border:1px solid #014769;
	border-left:none;
}

.left_top {
	float: left;
	border: 0px solid #283147;
	margin: 0px;
	padding: 0px;
	width: 75%;
	height: 70%;
}

.left_top iframe {
	border: 0px;
	margin: 0px;
	padding: 0px;
}

.left_down {
	float: left;
	border: 1px solid #283147;
	margin: 0px 10px 0px 13px;
	padding: 0px 0px 3px 0px;
	width: 74%;
	height: 24%;
	position: fixed;
	bottom: 15px;
	left: 0px;
	background: url(../../images/img/bg.png) 0px 0px repeat;
}
/*只针对火狐的写法*/
@-moz-document url-prefix() { .left_down { bottom:8px; } }
/* @media screen and (-webkit-min-device-pixel-ratio:0) { .left_down { bottom:8px; } } */
.right {
	float: right;
	margin: 0px 10px 0px 0px;
	width: 23%;
	height: 95%;
	position: fixed;
	right:0px;
}
.rightCon {
	width: 100%;
	height: 87%;
	position: absolute;
	bottom:65px;
	right: 0px;
}
/*针对屏幕分辨率大于1380的浏览器设置的样式*/
 @media only screen and (min-width: 1380px) {
 	.left_top {
		height: 70%;
	}
	.left_down {
		bottom:22px;
	}
	.rightCon .tabBox{
		margin-bottom:16px;
	}
   .rightCon {
		width: 100%;
		height: 95%;
		position: absolute;
		bottom:39px;
		right: 0px;
	}
 }
