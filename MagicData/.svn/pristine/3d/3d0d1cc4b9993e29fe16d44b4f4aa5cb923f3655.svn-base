@charset "utf-8";
/* CSS Document */
.topEmailCon{
	width:100%;
	overflow: hidden;
}
.emailidBox{
	width:100%;
}
.keywordBox .keyword,.addtagBox .addtag{
	display:inline-block;
	float:left;
	width:9%;
	text-align:right;
	line-height:30px;
}
.tagList{
	width:90%;
	text-align:left;
	float:left;
}
.tagList .tagB{
	padding:4px 10px;
	float:left;
	border-radius:5px;
	background:#F90;
	color:#fff;
	margin:0px 10px 10px 0px;
}
div.tagsinput{
	background:none;
	height:auto;
	border:none;
}
.addtagList{
	width:85%;
	float:left;
}
div.tagsinput input{
	border:1px solid #a5d24a;
}
/*轮播图*/
#imgBox{ 
	width: 100%;
    height:auto;
    margin-bottom:10px;
    overflow-x: auto;
	overflow-y: hidden;
	white-space:nowrap;
}
.imgDiv{
	width:100%;
	height:150px;
	margin-bottom:10px;
}
.listul{
	width:32%;
	height:150px;
	/* float:left; */
	display:inline-block;
	margin-right:10px;
	border:1px solid #ddd;
	overflow:hidden;
	white-space:nowrap;
}
.listul div{
	width:100%;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	line-height:24px;
	white-space:nowrap;  
}
.listul div span.titspan{
	width:20%;
	float:left;
}
.listul div .conP{
	width:80%;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	float:left;
}