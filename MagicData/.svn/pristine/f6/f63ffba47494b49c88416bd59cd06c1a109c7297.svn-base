<?xml version="1.0" encoding="utf-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.integrity.dataSmart.dataImport.bean">

     <class name="TDatabase" table="t_database">
         <id name="id" column="id">
             <generator class="identity" />
         </id>
         <property name="connectionName" column="connectionName" not-null="true" length="100" />
         <property name="connectionDB" column="connectionDB" not-null="true" length="100" />
         <property name="connectionServierName" column="connectionServierName" not-null="true" length="100" />
         <property name="connectionDBTableName" column="connectionDBTableName" not-null="true" length="100" />
         <property name="connectionPort" column="connectionPort" not-null="true" length="100" />
         <property name="connectionDBUserName" column="connectionDBUserName" not-null="true" length="100" />
         <property name="connectionDBPassword" column="connectionDBPassword" not-null="true" length="100" />
         <property name="connectionDriverName" column="connectionDriverName" not-null="true" length="100" />
         
     </class>
 </hibernate-mapping>
