#imgBox{ 
	width: 100%;
    height: 56px;
 }
.boxCont .preButtn,.boxCont .nextButtn {
    width: 30px;
    height: 30px;
    display: inline-block;
    float: left;
    margin-top: 17px;
    cursor: pointer;
    background: url(../img/switcher.png) 5px 6px no-repeat;
}
.boxCont .preButtn {
    margin-left: 7px;
}
.boxCont .nextButtn {
    float: right;
    margin-right: 12px;
    background-position: -35px 6px;
}
.imgList {
    width: 94%;
    float: left;
    height: 56px;
    overflow: hidden;
    margin-top: 10px;
    position: relative;
}
.imgList ul {
    float: left;
    height: 56px;
    width: 1800px;
}

.imgList li {
    float: left;
    width: 214px;
    height: 56px;
    position: relative; 
    margin-left: 13px;
}
.imgDiv .imgspan img{
	margin-top: 4px;
}
.imgDiv .divcontent {
	width: 70%;
	margin: 0px 0px 0px 10px;
}
.imgDiv p{
	width: 100%;
	margin-bottom: 0px;
	line-height: 22px;
	display:block;
	white-space:nowrap;
	overflow:hidden; 
	text-overflow:ellipsis;
	color: #5a5a5a;
	font-family:"Arial";
}
#GUN {
    width: 100%;
    height:60px;
    overflow: hidden;
	position:relative;
}
#GUN .imgDiv {
    float: left;
    width: 214px;
    height: 56px;
    position: relative; 
    margin:0px 4px ;
}
#GUN .imgDiv:hover{
	border: 2px solid #70c1ff;
}
#imgScroll2 {
	top:0px;
	left:0px;
    position: absolute;
    width: 10000px;
}
/*浮层*/
.hoverdiv{
	width: 210px;
	height: 25px;
	background: url(../img/fcBg.png) 0px 0px repeat-x;
	z-index: 999;
	left: 0px;
	top: 30px;
	position: absolute;
	-webkit-transform: scale(0);
	-ms-transform: scale(0);
	transform: scale(0); 
	-webkit-transition: all 0.4s cubic-bezier(0.31, 0.57, 0.68, 1.36);
    -ms-transition: all 0.4s cubic-bezier(0.31, 0.57, 0.68, 1.36);
    transition: all 0.4s ;
    z-index: 999999; 
    +display: none;
    display: none\9;
    cursor: pointer;
}
.imgDiv:hover .hoverdiv{
	-webkit-transform: scale(1);
	-ms-transform: scale(1);
	transform: scale(1); 
    -webkit-transition: all 0.4s cubic-bezier(0.31, 0.57, 0.68, 1.36);
    -ms-transition: all 0.4s cubic-bezier(0.31, 0.57, 0.68, 1.36);
    transition: all 0.4s ;
    +display: block;
    display: block\9;
}
.hoverdiv span{
	color:#fff;	
	padding-left: 30px;
	line-height: 24px;
	background: url(../img/topIcon.png) 0px 0px no-repeat;
}
.hoverdiv span.bjspan{
	float: left;
	background-position: 12px 7px;
}
.hoverdiv span.delspan{
	float: right;
	background-position: 12px -15px;
	margin-right: 20px;
}














