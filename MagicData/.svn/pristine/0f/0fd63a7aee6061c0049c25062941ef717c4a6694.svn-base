<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	

	<bean id="echartsBarDao" class="com.integrity.dataSmart.timeLine.dao.EchartsBarDao" >
	</bean>
	<bean id="echartsBarService" class="com.integrity.dataSmart.timeLine.service.EchartsBarService" >
		<property name="echartsBarDao" ref="echartsBarDao"/>
	</bean>
	<bean id="echartsBarAction" class="com.integrity.dataSmart.timeLine.action.EchartsBarAction" scope="prototype">
		<property name="echartsBarService" ref="echartsBarService"/>
	</bean> 
</beans>