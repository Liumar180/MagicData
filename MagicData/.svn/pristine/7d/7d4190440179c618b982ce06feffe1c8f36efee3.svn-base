<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.integrity.dataSmart.dataImport.bean.ImportMapping" table="impmapping" >
		<id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="500" >
                <comment>映射模板名称</comment>
            </column>
        </property>
        <property name="desc" type="java.lang.String">
            <column name="description" length="65535" >
                <comment>描述</comment>
            </column>
        </property>
        <property name="nodes" type="java.lang.String">
            <column name="nodes" length="65535" >
                <comment>结点</comment>
            </column>
        </property>
        <property name="edges" type="java.lang.String" >
            <column name="edges" length="65535" >
                <comment>边</comment>
            </column>
        </property>
        <property name="image" type="java.lang.String" >
            <column name="image" length = "16777216" >
                <comment>图片</comment>
            </column>
        </property>
        <property name="time" type="java.util.Date">
            <column name="time" >
                <comment>创建时间</comment>
            </column>
        </property>
	</class>
</hibernate-mapping>