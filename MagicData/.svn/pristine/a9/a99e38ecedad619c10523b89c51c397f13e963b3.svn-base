.a-upload {
    padding: 4px 10px;   
    line-height: 20px;
    position: relative;
    cursor: pointer;
    color: #fff;
    background: #0081d7;
    border: 1px solid #0081d7;
    border-radius: 4px;
    overflow: hidden;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    float: left;
}

.a-upload  input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);
    cursor: pointer
}

.a-upload:hover {
    color: #fff;
    background: #0081d7;
    border-color: #0081d7;
    text-decoration: none
}
