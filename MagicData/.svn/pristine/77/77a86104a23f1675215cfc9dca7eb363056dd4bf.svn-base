<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.integrity.system.auth.bean.AuthUser" table="authUser" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="64" not-null="true">
                <comment>用户名</comment>
            </column>
        </property>
         <property name="password" type="java.lang.String">
            <column name="password" length="64" not-null="true">
                <comment>密码</comment>
            </column>
        </property>
         <property name="name" type="java.lang.String">
            <column name="name" length="64" >
                <comment>姓名</comment>
            </column>
        </property>
         <property name="phone" type="java.lang.String">
            <column name="phone" length="64" >
                <comment>电话</comment>
            </column>
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="status" length="10" not-null="true">
                <comment>状态：0 启用，1 禁用</comment>
            </column>
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" length="250" >
                <comment>描述</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="createTime"  not-null="true">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="modifyTime" type="java.util.Date">
            <column name="modifyTime" >
                <comment>修改时间</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
