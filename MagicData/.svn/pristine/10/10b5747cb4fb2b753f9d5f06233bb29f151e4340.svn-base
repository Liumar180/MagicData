package com.integrity.dataSmart.dataImport.util;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
/**
 * 获取Titan结构工具类
 * @author RenSx
 *
 */
public class TitanStructureUtil {
	public static List<Map<String,Map<String,String>>> getTitanStructure(){
		/*  list中有两个元素 
	 	第一个为点的Map(key为节点的类型（type），value为节点的属性Map（key为属性名称，value为属性值类型标识；如果没有属性则为空map）)
	 	第二个为边的Map(key为边的标签（label），value为边的属性Map（key为属性名称，value为属性值类型标识；如果没有属性则为空map）)
		*/
		List<Map<String,Map<String,String>>> list = new ArrayList<Map<String,Map<String,String>>>();
		//点集合
		Map<String,Map<String,String>> vertex = new HashMap<String,Map<String,String>>();
		//边集合
		Map<String,Map<String,String>> edge = new HashMap<String,Map<String,String>>();
		
		list.add(vertex);
		list.add(edge);
		//Person
		Map<String,String> vertex_Person = new HashMap<String,String>();
		vertex_Person.put("name", DataType.STRING.getValue());
		vertex_Person.put("idcard", DataType.STRING.getValue());
		vertex_Person.put("country", DataType.STRING.getValue());
		vertex_Person.put("sex",DataType.STRING.getValue());
		vertex_Person.put("birthday", DataType.STRING.getValue());
		vertex.put("Person",vertex_Person);
		//Phone
		Map<String,String> vertex_Phone = new HashMap<String,String>();
		vertex_Phone.put("model", DataType.STRING.getValue());
		vertex_Phone.put("phonenum", DataType.STRING.getValue());
		vertex.put("Phone", vertex_Phone);
		//Resume
		Map<String,String> vertex_Resume = new HashMap<String,String>();
		vertex_Resume.put("title", DataType.STRING.getValue());
		vertex_Resume.put("keywords", DataType.STRING.getValue());
		vertex_Resume.put("time", DataType.LONG.getValue());
		vertex.put("Resume", vertex_Resume);
		//Account
		Map<String,String> vertex_Account = new HashMap<String,String>();
		vertex_Account.put("domain", DataType.STRING.getValue());
		vertex_Account.put("uid", DataType.STRING.getValue());
		vertex_Account.put("username", DataType.STRING.getValue());
		vertex_Account.put("password",DataType.STRING.getValue());
		vertex_Account.put("email", DataType.STRING.getValue());
		vertex_Account.put("createtime", DataType.LONG.getValue());
		vertex_Account.put("phonenum", DataType.STRING.getValue());
		vertex_Account.put("regip", DataType.STRING.getValue());
		vertex_Account.put("question", DataType.STRING.getValue());
		vertex.put("Account", vertex_Account);
		//Email
		Map<String,String> vertex_Email = new HashMap<String,String>();
		vertex_Email.put("email", DataType.STRING.getValue());
		vertex_Email.put("password", DataType.STRING.getValue());
		vertex.put("Email", vertex_Email);
		//Location
		Map<String,String> vertex_Location = new HashMap<String,String>();
		vertex_Location.put("address", DataType.STRING.getValue());
		vertex_Location.put("place", DataType.STRING.getValue());
		vertex.put("Location", vertex_Location);
		//IM
		Map<String,String> vertex_IM = new HashMap<String,String>();
		vertex_IM.put("domain", DataType.STRING.getValue());
		vertex_IM.put("numid", DataType.STRING.getValue());
		vertex_IM.put("nickname", DataType.ARRAY.getValue());
		vertex.put("IM", vertex_IM);
		//Group
		Map<String,String> vertex_Group = new HashMap<String,String>();
		vertex_Group.put("groupnum", DataType.STRING.getValue());
		vertex_Group.put("createtime", DataType.LONG.getValue());
		vertex_Group.put("ownerqq", DataType.STRING.getValue());
		vertex_Group.put("groupname", DataType.STRING.getValue());
		vertex_Group.put("groupdesc", DataType.STRING.getValue());
		//CallEvent
		Map<String,String> vertex_CallEvent = new HashMap<String,String>();
		vertex_CallEvent.put("from", DataType.STRING.getValue());
		vertex_CallEvent.put("to", DataType.STRING.getValue());
		vertex_CallEvent.put("time", DataType.LONG.getValue());
		vertex_CallEvent.put("long", DataType.LONG.getValue());
		vertex.put("CallEvent",vertex_CallEvent);
		//LoginEvent
		Map<String,String> vertex_LoginEvent = new HashMap<String,String>();
		vertex_LoginEvent.put("domain", DataType.STRING.getValue());
		vertex_LoginEvent.put("username", DataType.STRING.getValue());
		vertex_LoginEvent.put("ip", DataType.STRING.getValue());
		vertex_LoginEvent.put("time", DataType.LONG.getValue());
		vertex.put("LoginEvent",vertex_LoginEvent);
		//EmailEvent
		Map<String,String> vertex_EmailEvent = new HashMap<String,String>();
		vertex_EmailEvent.put("title", DataType.STRING.getValue());
		vertex_EmailEvent.put("content", DataType.STRING.getValue());
		vertex_EmailEvent.put("from", DataType.STRING.getValue());
		vertex_EmailEvent.put("to", DataType.STRING.getValue());
		vertex_EmailEvent.put("time", DataType.LONG.getValue());
		vertex_EmailEvent.put("messageId", DataType.STRING.getValue());
		vertex.put("EmailEvent", vertex_EmailEvent);
		//StayEvent
		Map<String,String> vertex_StayEvent = new HashMap<String,String>();
		vertex_StayEvent.put("hotelid", DataType.STRING.getValue());
		vertex_StayEvent.put("hotelname", DataType.STRING.getValue());
		vertex_StayEvent.put("orderno", DataType.STRING.getValue());
		vertex_StayEvent.put("arrivedate", DataType.LONG.getValue());
		vertex_StayEvent.put("departuredate", DataType.LONG.getValue());
		vertex_StayEvent.put("roomno", DataType.STRING.getValue());
		vertex_StayEvent.put("hotelflag", DataType.STRING.getValue());
		vertex.put("StayEvent", vertex_StayEvent);
		
		//own
		Map<String,String> edge_own = new HashMap<String,String>();
		edge.put("own",edge_own);
		//callfrom
		Map<String,String> edge_callfrom = new HashMap<String,String>();
		edge_callfrom.put("eventtime", DataType.LONG.getValue());
		edge.put("callfrom",edge_callfrom);
		//callto
		Map<String,String> edge_callto = new HashMap<String,String>();
		edge_callto.put("eventtime", DataType.LONG.getValue());
		edge.put("callto",edge_callto);
		//login
		Map<String,String> edge_login = new HashMap<String,String>();
		edge_login.put("eventtime", DataType.LONG.getValue());
		edge.put("login", edge_login);
		//emailfrom
		Map<String,String> edge_emailfrom = new HashMap<String,String>();
		edge_emailfrom.put("eventtime", DataType.LONG.getValue());
		edge.put("emailfrom", edge_emailfrom);
		//emailto
		Map<String,String> edge_emailto = new HashMap<String,String>();
		edge_emailto.put("eventtime", DataType.LONG.getValue());
		edge.put("emailto", edge_emailto);
		//emailcc
		Map<String,String> edge_emailcc = new HashMap<String,String>();
		edge_emailcc.put("eventtime", DataType.LONG.getValue());
		edge.put("emailcc", edge_emailcc);
		//relational
		Map<String,String> edge_relational = new HashMap<String,String>();
		edge_relational.put("relationtype", DataType.STRING.getValue());
		edge_relational.put("nickname", DataType.STRING.getValue());
		edge.put("relational", edge_relational);
		//stay
		Map<String,String> edge_stay = new HashMap<String,String>();
		edge_stay.put("eventtime", DataType.LONG.getValue());
		edge.put("stay", edge_stay);
		//cohabit
		Map<String,String> edge_cohabit = new HashMap<String,String>();
		edge.put("cohabit", edge_cohabit);
		//group
		Map<String,String> edge_group = new HashMap<String,String>();
		edge_group.put("grouptype", DataType.STRING.getValue());
		edge_group.put("nickname", DataType.STRING.getValue());
		edge.put("group",edge_group);
		return list;
	}
}

enum DataType{
	LONG("long", "long"),
	STRING("string","string"),
	ARRAY("array","array");
	
	private String name;

	private String value;

	private DataType(String name, String value) {
		this.name = name;
		this.value = value;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
