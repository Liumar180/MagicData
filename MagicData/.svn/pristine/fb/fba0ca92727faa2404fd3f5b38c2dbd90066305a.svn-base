html,body{
	width: 100%;
	height: 100%;
	font-family: "微软雅黑";
	background: url(../../../images/systemManage/bodyBg.jpg) top center no-repeat;
	/*overflow: hidden;*/
}
.navbar{
	min-height: 60px;
	margin-bottom: 0px;
	color: #fff;
}
.navbar-link{
	color: #fff;
	margin-left: 8px;
}
.container{
	width: 100%;
	height: 100%;
}
@media (min-width: 1366px) {
  .container {
    width: 100%;
  }
}
.mainCon{
	min-width:1280px;
	height: 88%;
	padding: 20px 0px 0px 0px;
}
.leftCon{
	width: 18%;
	height: 100%;
	border:2px solid #1141b0;	
	-webkit-box-shadow:inset 0 0 25px #1141b0;  
  	-moz-box-shadow:inset 0 0 25px #1141b0;  
  	box-shadow:inset 0 0 25px #1141b0; 
  	border-radius: 4px;
  	padding: 10px;
}
.leftInner1{
	width: 98%;
	height: 100%;
	border: 1px solid #000000;
	margin: 0 auto;
}
.leftInner2{
	width: 99%;
	height: 100%;
	border: 1px solid #182fba;
	margin: 0 auto;
	padding-top: 30px;
}
.leftInner2 ul li{
	margin-bottom: 20px;
}
.leftInner2 ul li a{
	text-align: center;
	line-height: 50px;
	background: url(../../../images/systemManage/leftIcon.png) 22px 11px no-repeat;
	display: block;
}
.leftInner2 ul li a{
	color: #fff;
	font-size: 14px;
}
.leftInner2 ul li.role a{
	background-position: 22px -63px;
}
.leftInner2 ul li.log a{
	background-position: 22px -139px;
}
.leftInner2 ul li.object a{
	background-position: 22px -215px;
}
.leftInner2 ul li.data a{
	background-position: 22px -290px;
}
.leftInner2 ul li.reset a{
	background-position: 22px -363px;
}
.leftInner2 ul li:hover,.leftInner2 ul li.setOn{
	background: #2a3fc1;
}
@media (min-width: 1400px) {
  .leftInner2 ul li a{
	text-align: center;
	line-height: 50px;
	background: url(../../../images/systemManage/leftIcon.png) 50px 11px no-repeat;
	display: inline-block;
	padding-left:100px;
	}
	.leftInner2 ul li.role a{
		background-position: 50px -63px;
	}
	.leftInner2 ul li.log a{
		background-position: 50px -139px;
	}
	.leftInner2 ul li.object a{
		background-position: 50px -215px;
	}
	.leftInner2 ul li.data a{
		background-position: 50px -290px;
	}
	.leftInner2 ul li.reset a{
		background-position: 50px -363px;
	}
}
.rightCon{
	width: 80%;
	height: 100%;
	border:2px solid #1141b0;	
	-webkit-box-shadow:inset 0 0 25px #1141b0;  
  	-moz-box-shadow:inset 0 0 25px #1141b0;  
  	box-shadow:inset 0 0 25px #1141b0; 
  	border-radius: 4px;
  	padding: 15px;
}
/*tab*/
.userconBox{
	height: 100%;
	padding: 0px 5px 10px 5px;
	border: 2px solid #1b31b6;
	overflow-y: auto;
}
.topbar{
	width: 100%;
	height: 50px;
	margin: 0 auto;
	background: url(../../../images/systemManage/topbarBg.png) 0px 0px repeat-x;
}
.topbar li{
	float: left;
	margin-right: 10px;
}

.topbar ul li a{
	text-align: center;
	line-height: 50px;
	background: url(../../../images/systemManage/listIcon.png) 10px 14px no-repeat;
	display: inline-block;
	padding-left: 40px;
}
.topbar ul li a{
	color: #d6e7fa;
}
.topbar ul li.delete a{
	background-position: 14px -61px;
}
/*日志管理*/
.topbar .form-group{
	width: 23%;
	float: left;
	margin-bottom: 0px;
}
.topbar .form-group .labelTit{
	width: 38%;
	text-align: right;
	float: left;
	font-weight: normal;
	color:#fff;
	line-height: 50px;
	margin-bottom: 0px;
	text-align: center;
}
.topbar .form-control{
	width: 62%;
	margin-top: 7px;
	line-height: 30px;
	border: 1px solid #2548a7;
	background: #142a65;
}
/*搜索按钮*/
.searchDiv{
	border: 1px solid #3b66dd;
	background: #264aa6;
	 border-radius: 5px;
    color: #fff;
    line-height: 30px;
    padding: 0 10px;
    margin: 7px 0px 0px 10px;
    cursor: pointer;
}
/*角色树*/
.treeDiv{
	float: left;
	width: 56%;
	margin-bottom: 0px;
	height:245px;
	overflow-x:hidden;
	overflow-y:auto;
}
/*----添加对象弹出层----*/
.addobjectCon .form-group{
	width: 45%;
}
.addobjectCon .form-control{
	width: 68%;
}
.addobject,.delObject{
	width: 40px;
	height: 26px;
	float: left;
	background: url(../../../images/systemManage/ryIcon.png) 0px 0px no-repeat;
	margin-top: 8px;
	cursor: pointer;
}
.delObject{
	background-position: 8px -77px;
}
/**/
.hideDiv .checkInput{
	display:inline-block;
	float:left;
	margin-top:12px;
	margin-right:4px;
}
/*12月25加--用户管理*/
.addCon .comDiv{
	width:100%;
}
.comDiv .labelTit{
	width:10%;
}
.comDiv textarea{
	width:78%;
}
.comDiv .usertreeDiv{
	width:78%;
	height:195px;
}









