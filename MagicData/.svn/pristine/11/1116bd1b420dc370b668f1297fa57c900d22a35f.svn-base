package com.integrity.dataSmart.titanGraph.dao;

import org.springframework.jdbc.core.support.JdbcDaoSupport;

public class SearchQqRelationDao extends JdbcDaoSupport{
	
	public String queryQQlist(){
		String sql  ="SELECT c.QQNUM,c.FRIENDREMARK  FROM ( SELECT s.QQNUM,s.FRIENDREMARK, ROWNUM RN  " +
				"FROM (SELECT n.QQNUM qqnum,n.FRIENDREMARK friendremark from TB_RNS_QQ_NAME n where ROWNUM < 20) s) c WHERE RN >=0";
		this.getJdbcTemplate().execute(sql);
		return "测试";
	}


}
