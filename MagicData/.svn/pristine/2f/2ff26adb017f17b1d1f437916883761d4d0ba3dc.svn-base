<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.integrity.dataSmart.pwdCrack.bean.PwdTask" table="pwd_task" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="uuid" type="java.lang.String">
            <column name="uuid" length="64" not-null="true">
                <comment>UUID</comment>
            </column>
        </property>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="64" not-null="true">
                <comment>用户名</comment>
            </column>
        </property>
        <property name="pwdEncrypt" type="java.lang.String">
            <column name="pwdEncrypt" length="256" not-null="true">
                <comment>加密的密码</comment>
            </column>
        </property>
        <property name="pwdCrack" type="java.lang.String">
            <column name="pwdCrack" length="64">
                <comment>解密后的密码</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="createTime" length="10" sql-type="dateTime">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="runStatus" type="java.lang.Integer">
            <column name="runStatus" not-null="true">
                <comment>任务状态2表示正在执行，1表示未执行，0表示执行完成，-1表示执行失败</comment>
            </column>
        </property>
        <property name="runPercent" type="java.lang.Integer">
            <column name="runPercent">
                <comment>正在执行的任务状态的百分比</comment>
            </column>
        </property>
        </class>
</hibernate-mapping>
