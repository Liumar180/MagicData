<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 查询<result name="success" type="json"></result>  -->
	<!-- 数据库连接 配置页面 -->
	<package name="pwdCrack" extends="base" namespace="/pwdCrack">
        <action name="getPTList" class="pwdTaskAction" method="getPTList"> 
	        <result name="success">
	      		/WEB-INF/jsp/pwdCrack/pwdCrackList.jsp
	      	</result>
        </action>
        </package>
      <package name="ajax_pwdCrack" extends="jsondefault" namespace="/ajax_pwdCrack">
		<!-- 获取任务队列 -->
		<action name="getPTListByPage" class="pwdTaskAction" method="getPTListByPage">
			<result name="success" type="json"></result>
		</action>
		<action name="deletePTById" class="pwdTaskAction" method="deletePTById">
			<result name="success" type="json"></result>
		</action>
		<action name="addPwdTask" class="pwdTaskAddAction" method="addPwdTask">
			<result name="success" type="json"></result>
		</action>
		<action name="runPwdTask" class="pwdTaskAction" method="runPwdTask">
			<result name="success" type="json"></result>
		</action>
		<action name="getRunningPencent" class="pwdTaskAction" method="getRunningPencent">
			<result name="success" type="json"></result>
		</action>
		</package>
</struts>
