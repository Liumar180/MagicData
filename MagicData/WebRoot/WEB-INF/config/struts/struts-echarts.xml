<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.i18n.encoding" value="UTF-8" />	<constant name="struts.multipart.maxSize" value="90000000"/>

	<!-- 查询 -->
	<package name="echarts" extends="base" namespace="/bar">
	   <action name="queryAllNodes" method="queryAllNodes" class="echartsBarAction">   
   			<result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
        	</result>
        </action> 
	
         <action name="queryYearsById" method="queryYearsById" class="echartsBarAction">   
   			<result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
        	</result>
        </action> 
        
        <action name="queryLastYears" method="queryLastYears" class="echartsBarAction">   
       		<result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result>
        </action> 
        
         <action name="queryNextYears" method="queryNextYears" class="echartsBarAction"> 
         	<result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result>  
        </action> 
        
        <action name="queryMonth" method="queryMonth" class="echartsBarAction"> 
            <result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result> 
        </action>  
        
        <action name="queryDays" method="queryDays" class="echartsBarAction"> 
            <result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result> 
        </action>  
        
        <action name="queryEmailById" method="queryEmailById" class="echartsBarAction"> 
	        <result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result> 
        </action> 
        <action name="getFilterkeywords" method="getFilterkeywords" class="echartsBarAction"> 
	        <result name="success" type="stream">  
	            <param name="contentType">application/octet-stream; charset=utf-8</param>  
	            <param name="inputName">inputStream</param>  
	        </result> 
        </action> 
       <action name="downLoadEmail" method="downLoadEmail" class="echartsBarAction"> 
	        <result name="downloadok" type="stream">
			     <param name="contentType">application/octet-stream</param>
			     <param name="inputName">inputStream</param>
			     <param name="contentDisposition">attachment;filename="${fileName}"</param>
			     <param name="buffersize">8192</param>   
			 </result>  
        </action> 
        
	</package> 
	
	
</struts>
