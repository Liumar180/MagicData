<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	
	 
	<bean id="testDatabaseConAction" class="com.integrity.dataSmart.dataImport.action.TestDatabaseConAction" scope="prototype">
		<property name="dBService" ref="dBService"/>
		<property name="addForm">
			<ref bean="dImpAddForm" />
		</property>
		<property name="dataImpAddService">
			<ref bean="dataImpAddService" />
		</property>
		<property name="dataImpTaskService">
			<ref bean="dataImpTaskService" />
		</property>
	</bean>
	<bean id="dBDao" class="com.integrity.dataSmart.dataImport.dao.DBDao">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>

    <bean id="dBService" class="com.integrity.dataSmart.dataImport.service.DBService">
        <property name="dBDao" ref="dBDao"/>
    </bean>
	 <bean id="jDBCutil" class="com.integrity.dataSmart.util.importData.JDBCutil">
    </bean>
    
    <!-- =========== add by HanXue start =========== -->
    <!-- 数据导入任务新增页面Form对象 -->
    <bean id="dImpAddForm"
		class="com.integrity.dataSmart.dataImport.pojo.DataImpAddForm" scope="prototype">
	</bean>
	<!-- 数据导入任务新增Session用对象 -->
	<bean id="dImpAddPojo"
		class="com.integrity.dataSmart.dataImport.pojo.DataImpAddPojo" scope="prototype">
	</bean>
	<!-- 数据导入任务数据操作Dao -->
	<bean id="dbImpTaskDao" class="com.integrity.dataSmart.dataImport.dao.DataImpTaskDao">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
    <!-- 数据导入任务新增Service -->
	<bean id="dataImpAddService"
		class="com.integrity.dataSmart.dataImport.service.DataImpAddService">
		<property name="dbImpTaskDao"><ref bean="dbImpTaskDao"/></property>
		<property name="dbService">
			<ref bean="dBService" />
		</property>
	</bean>
	<!-- 数据导入任务查询、分页、删除、查看信息、执行等Service -->
	<bean id="dataImpTaskService"
		class="com.integrity.dataSmart.dataImport.service.DataImpTaskService">
		<property name="dbImpTaskDao"><ref bean="dbImpTaskDao"/></property>
		<property name="dBDao"><ref bean="dBDao"/></property>
		<property name="dataImpAddService">
			<ref bean="dataImpAddService" />
		</property>
	</bean>
	<!-- 数据导入任务新增Action -->
    <bean id="dImpAddAction"
		class="com.integrity.dataSmart.dataImport.action.DataImpAddAction" scope="prototype">
		<property name="dBService">
			<ref bean="dBService" />
		</property>
		<property name="addForm">
			<ref bean="dImpAddForm" />
		</property>
		<property name="dataImpAddService">
			<ref bean="dataImpAddService" />
		</property>
	</bean>
	<!-- 数据导入任务新增Ajax校验用Action -->
	<bean id="dataImpAddAjaxAction"
		class="com.integrity.dataSmart.dataImport.action.DataImpAddAjaxAction" scope="prototype">
		<property name="dataImpAddService">
			<ref bean="dataImpAddService" />
		</property>
		<property name="dBService">
			<ref bean="dBService" />
		</property>
	</bean>
	<!-- 数据导入任务查询、分页、删除、查看信息、执行等Action -->
	<bean id="dataImpTaskAction"
		class="com.integrity.dataSmart.dataImport.action.DataImpTaskAction" scope="prototype">
		<property name="dataImpTaskService">
			<ref bean="dataImpTaskService" />
		</property>
	</bean>
	<!-- =========== add by HanXue end =========== -->
	<!-- =========== add by RenSx start =========== -->
	<bean id="dataManageAction" class="com.integrity.dataSmart.dataImport.action.DataManageAction" scope="prototype">
		<property name="dataManageService"><ref bean="dataManageService" /></property>
		<property name="dataImpAddService"><ref bean="dataImpAddService" /></property>
	</bean>
	<bean id="dataManageService" class="com.integrity.dataSmart.dataImport.service.DataManageService">
		<property name="dBDao" ref="dBDao"/>
	</bean>
	<bean id="titanStruAction" class="com.integrity.dataSmart.dataImport.action.TitanStruAction" scope="prototype">
		
	</bean>
	<bean id="importMappingAction" class="com.integrity.dataSmart.dataImport.action.ImportMappingAction" scope="prototype">
		<property name="importMappingService"><ref bean="importMappingService" /></property>
	</bean>
	<bean id="importMappingService" class="com.integrity.dataSmart.dataImport.service.ImportMappingService">
		<property name="importMappingDao" ref="importMappingDao"/>
	</bean>
	<bean id="importMappingDao" class="com.integrity.dataSmart.dataImport.dao.ImportMappingDao">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
    <bean id="csvDataManageAction" class="com.integrity.dataSmart.dataImport.action.CsvDataManageAction" scope="prototype">
		<property name="csvDataService"><ref bean="csvDataService" /></property>
	</bean>
	<bean id="csvDataService" class="com.integrity.dataSmart.dataImport.service.CsvDataService">
		<property name="csvDataDao" ref="csvDataDao"/>
	</bean>
	<bean id="csvDataDao" class="com.integrity.dataSmart.dataImport.dao.CsvDataDao">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
	<!-- =========== add by RenSx end =========== -->

</beans>