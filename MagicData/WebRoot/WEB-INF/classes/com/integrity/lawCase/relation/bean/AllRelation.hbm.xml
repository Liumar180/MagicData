<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.integrity.lawCase.relation.bean.AllRelation" table="all_ralation" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="type" length="64" not-null="true">
                <comment>关联类型  从ConstantManage中取值</comment>
            </column>
        </property>
        <property name="typeId" type="java.lang.Long">
            <column name="typeId" length="20" not-null="true">
                <comment>关联的主id</comment>
            </column>
        </property>
        <property name="caseIds" type="java.lang.String">
            <column name="caseIds" length="512" >
                <comment>案件ids</comment>
            </column>
        </property>
        <property name="fileIds" type="java.lang.String">
            <column name="fileIds" length="512" >
                <comment>文件ids</comment>
            </column>
        </property>
        <property name="hostIds" type="java.lang.String">
            <column name="hostIds" length="512" >
                <comment>主机ids</comment>
            </column>
        </property>
        <property name="organizationIds" type="java.lang.String">
            <column name="organizationIds" length="512" >
                <comment>组织ids</comment>
            </column>
        </property>
        <property name="peopleIds" type="java.lang.String">
            <column name="peopleIds" length="512" >
                <comment>人员ids</comment>
            </column>
        </property>
        
        <property name="backup1" type="java.lang.String">
            <column name="backup1" length="64" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="backup2" type="java.lang.String">
            <column name="backup2" length="64" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="backup3" type="java.lang.Integer">
            <column name="backup3" >
                <comment>备用</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
