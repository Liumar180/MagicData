<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.integrity.lawCase.caseManage.bean.WorkRecord" table="workRecord" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
         <property name="caseId" type="java.lang.Long">
            <column name="caseId" length="20" >
                <comment>案件id</comment>
            </column>
        </property>
        <property name="title" type="java.lang.String">
            <column name="title" length="1000" not-null="true">
                <comment>标题</comment>
            </column>
        </property>
        <property name="conent" type="java.lang.String">
            <column name="conent" length="65535" >
                <comment>内容</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="createTime"  not-null="true">
                <comment>创建时间</comment>
            </column>
        </property>
        
        <property name="by1" type="java.lang.String">
            <column name="by1" length="64" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="by2" type="java.lang.String">
            <column name="by2" length="4000" >
                <comment>备用</comment>
            </column>
        </property>
        <property name="by3" type="java.lang.Integer">
            <column name="by3" >
                <comment>备用</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
